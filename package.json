{
  "name": "azi-utils",
  "version": "0.1.1",
  "description": "Common tool function and styles encapsulation",
  "main": "dist/core/index.js",
  "module": "dist/core/index.mjs",
  "types": "dist/core/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/core/index.mjs",
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./core": {
      "import": "./dist/core/index.mjs",
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./directives": {
      "import": "./dist/directives/index.mjs",
      "types": "./dist/directives/index.d.ts"
    }
  },
  "packageManager": "pnpm@7.9.5",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "pnpm clean && rollup -c",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:preview": "vite preview --outDir ./html",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "web",
    "ts",
    "utils",
    "development",
    "tools"
  ],
  "author": {
    "name": "azi",
    "email": "azi0427@163.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.7.13",
    "@vitest/coverage-v8": "^0.34.1",
    "@vitest/ui": "^0.34.1",
    "axios": "^1.1.3",
    "core-js": "^3.25.0",
    "happy-dom": "^10.9.0",
    "rimraf": "^3.0.2",
    "rollup": "2.68.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "typescript": "^4.8.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "axios": "^1.1.3"
  },
  "dependencies": {
    "crypto-js": "^4.1.1"
  }
}
